{
  "version": "0.2",
  "language": "en-GB,es-ES",
  "import": [
    "@cspell/dict-en-gb/cspell-ext.json",
    "@cspell/dict-es-es/cspell-ext.json"
  ],
  "useGitignore": true,
  "ignorePaths": [
    ".github",
    ".next",
    ".turbo",
    ".vscode",
    "node_modules",
    "out",
    "public",
    "coverage",
    "pnpm-lock.yaml",
    "tsconfig.tsbuildinfo"
  ],
  "allowCompoundWords": false,
  "enableFiletypes": [
    "cjs",
    "css",
    "json",
    "jsonc",
    "md",
    "mjs",
    "ts",
    "tsx",
    "yaml"
  ],
  "languageSettings": [
    {
      "languageId": "markdown",
      "dictionaries": ["en-gb", "es-es", "es-ar"],
      "caseSensitive": false
    },
    {
      "languageId": [
        "typescript",
        "typescriptreact",
        "javascript",
        "javascriptreact"
      ],
      "dictionaries": ["en-gb", "es-es", "es-ar"]
    }
  ],
  "words": [
    "Apoyá",
    "boomershooter",
    "caece",
    "carcinisation",
    "Carcinisation",
    "Comprame",
    "Gridshare",
    "Grotesk",
    "itchio",
    "linkedin",
    "Linkedin",
    "mantenibilidad",
    "marcotoniut",
    "Mentoría",
    "modelcontextprotocol",
    "modularidad",
    "Moixa",
    "noai",
    "noarchive",
    "nocontentai",
    "noimageai",
    "noimageindex",
    "nojekyll",
    "overengineering",
    "plex",
    "Ponete",
    "Satoshi",
    "Segoe",
    "sénior",
    "Shadcn",
    "Stefano",
    "styleguide",
    "Supabase",
    "Toniut",
    "typesafe",
    "Typesafe",
    "udemy",
    "Vars",
    "vercel",
    "xxxl",
    "zelt",
    "zipline"
  ]
}
