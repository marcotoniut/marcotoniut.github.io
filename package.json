{
  "name": "marcotoniut.github.io",
  "private": true,
  "scripts": {
    "build": "next build",
    "deploy": "gh-pages -d out -t",
    "dev": "pnpm generate && concurrently \"pnpm generate:watch\" \"next dev\"",
    "dev:pdf": "cross-env NEXT_PUBLIC_PDF_PRINTING=true next -p 3001",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format",
    "generate": "pnpm typesafe-i18n",
    "generate:watch": "pnpm typesafe-i18n:watch",
    "predeploy": "rm -rf node_modules/.cache && rm -rf out/ && pnpm build && touch out/.nojekyll",
    "print-cv": "pnpm node scripts/print-cv.js",
    "start": "next start",
    "typecheck": "tsc --pretty --noEmit",
    "typesafe-i18n": "typesafe-i18n --no-watch",
    "typesafe-i18n:watch": "typesafe-i18n"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.17.4",
    "date-fns": "^3.3.1",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typesafe-i18n": "^5.26.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@vanilla-extract/next-plugin": "^2.3.2",
    "child_process": "^1.0.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "puppeteer": "^21.7.0",
    "typescript": "4.5.2"
  },
  "packageManager": "pnpm@10.18.1"
}
