{
  "name": "marcotoniut.github.io",
  "private": true,
  "scripts": {
    "build": "next build --webpack",
    "deploy": "gh-pages -d out --nojekyll",
    "dev": "pnpm generate && concurrently \"pnpm generate:watch\" \"next dev --webpack\"",
    "dev:pdf": "cross-env NEXT_PUBLIC_PDF_PRINTING=true next dev --webpack -p 3001",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format",
    "generate": "pnpm typesafe-i18n",
    "generate:watch": "pnpm typesafe-i18n:watch",
    "predeploy": "rm -rf node_modules/.cache && rm -rf out/ && pnpm build",
    "print-cv": "pnpm node scripts/print-cv.js",
    "start": "next start",
    "test": "",
    "typecheck": "tsc --pretty --noEmit",
    "typesafe-i18n": "typesafe-i18n --no-watch",
    "typesafe-i18n:watch": "typesafe-i18n"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/recipes": "^0.5.7",
    "date-fns": "^4.1.0",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typesafe-i18n": "^5.26.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@vanilla-extract/next-plugin": "^2.3.2",
    "child_process": "^1.0.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "10.1.0",
    "gh-pages": "^6.1.1",
    "puppeteer": "^24.24.0",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.18.1"
}
